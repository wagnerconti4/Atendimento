    <table class="listaFuncionario">
        <tr>
            <td colspan="5"><h3>Lista de funcionarios</h3></td>
        </tr>
        <tr>
            <th>id</th>
            <th>Nome</th>
            <th>Telefone</th>
            <th>Email</th>
            <th>Data de criação</th>
        </tr>
        {{#each funcionarios}}
        <tr>
    <td><a href="http://localhost:3000/Atualiza_Funcionario/{{dataValues.id}}">{{dataValues.id}}</a></td>
   <td>{{dataValues.nome_funcionario}}</td>
   <td>{{dataValues.telefone_funcionario}}</td>
   <td>{{dataValues.email_funcionario}}</td>
   <td>{{dataValues.createdAt}} </td>
   </tr>
   {{/each}}
   </table>

