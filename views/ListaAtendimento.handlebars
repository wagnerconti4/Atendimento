<table> 
       <tr>
           <th colspan="3"><h3>Lista de atendimento</h3></th>
       </tr>
        <tr>
            <th>ID</th>
            <th>Status</th>
            <th>Nome</th>
            <th>Empresa</th>
            <th>Problema</th>
            <th>Telefone</th>
            <th>Logradouro</th>
            <th>Número</th>
            <th>Complemento</th>
            <th>Funcionário</th>
        </tr>
        <tr>
            {{#each atendimentos}}  
            <td><a href="http://localhost:8000/Atualiza_Atendimento/{{dataValues.id}}">{{dataValues.id}}</a></td>
            <td>{{dataValues.status}}</td>
            <td>{{dataValues.nome_cliente}}</td>
            <td>{{dataValues.nome_empresa}}</td>
            <td>{{dataValues.problema_cliente}}</td>
            <td>{{dataValues.telefone_cliente}}</td>
            <td>{{dataValues.endereco_cliente_rua}}</td>
            <td>{{dataValues.endereco_cliente_numero}}</td>
            <td>{{dataValues.endereco_cliente_complemento}}</td>
           {{#each funcionarios}}<td>{{dataValues.nome_funcionario}}</td>{{/each}}
        </tr>
        {{/each}}
    </table>  